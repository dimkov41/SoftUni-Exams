<nav>
    <a class="active" href="#/">Home</a>
    {{#if loggedIn}}
        <a href="#/allListings">All Listings</a>
        <a href="#/myListings">My Listings</a>
        <a href="#/createListing">Create Listing</a>
        <div id="profile">
            <a>Welcome {{username}}</a>
            <a href="#/logout">logout</a>
        </div>
    {{/if}}
</nav>


<div class="listing-details">
    <div class="my-listing-details">
        <p id="auto-title">{{{data.title}}}</p>
        <img src="{{data.imageUrl}}">
        <div class="listing-props">
            <h2>Brand: {{{data.brand}}}</h2>
            <h3>Model: {{{data.model}}}</h3>
            <h3>Year: {{{data.year}}}</h3>
            <h3>Fuel: {{{data.fuelType}}}</h3>
            <h3>Price: {{{data.price}}}</h3>
        </div>
        <div class="listings-buttons">

            <a href="#/edit/{{data._id}}" class="button-list">Edit</a>
            <a href="#/delete/{{data._id}}" class="button-list">Delete</a>


        </div>
        <p id="description-title">Description:</p>
        <p id="description-para">{{{data.description}}}</p>

    </div>
</div>

<div class="footer">
    <p>Created by MarikMayhem</p>
</div>